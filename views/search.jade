extends layout

block content
  div(role='main')
    .page.innerPage#searchPage(data-cache='never',data-role="page",data-url='#{url}')
      .topGrahic
      a.menuButton(href="/")
        img(src="/img/menu-button.png",alt="Menu")

      .padpage.padtop
        h1 
          a.back.ui-link(href='/')
            img(src='/img/icon-back.png',alt='<-')
            | Site Search

        .projects
          ul#autocomplete(data-role='listview',data-inset="true",data-filter="true",data-filter-placeholder="Search")
