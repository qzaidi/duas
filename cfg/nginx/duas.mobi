server {
    server_name duas.mobi;
  
    access_log /var/log/nginx/duas-nssl.access.log;
    error_log  /var/log/nginx/duas-nssl.error.log;

    return 301 https://$host$request_uri;
}

server {
    server_name www.duas.mobi;
  
    access_log /var/log/nginx/duas-nssl.access.log;
    error_log  /var/log/nginx/duas-nssl.error.log;

    return 301 https://duas.mobi$request_uri;
}
